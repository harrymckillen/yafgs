extends ../templates/html.jade

block title
  title %-Column Nudge Right | Yet Another Feckin' Grid System

block content
  div.the-grid

    include ../templates/mixins/columns.jade

    h1 %-Column Nudge Right
    - for (var z = 1; z < 100; z++)
      div.a-row
        div(class="a-column width-12-in-pc nudge-pc-right-"+z)
          div.content % Nudge Right #{z}%
